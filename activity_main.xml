<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity" >

    <!--   <SearchView
        android:id="@+id/searchView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true" >
    </SearchView> -->

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="68dp"
        android:text="@string/hello_world"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/TextView01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/textView1"
        android:layout_below="@+id/textView1"
        android:text="@string/text"
        android:textSize="12sp" />

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/TextView01"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="58dp" >

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="top|left"
            android:padding="0dp"
            android:paddingTop="0dp" >

            <Button
          	android:id="@+id/CreateInterview"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
 		        android:layout_alignParentLeft="true"
 		        android:layout_below="@+id/TextView01"
 		        android:layout_marginLeft="23dp"
        		android:layout_marginTop="40dp"
        		android:text="Create Interview" />
            <Button
        		android:id="@+id/CreatePoll"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
 		        android:layout_alignParentLeft="true"
 		        android:layout_below="@+id/TextView01"
 		        android:layout_marginLeft="23dp"
        		android:layout_marginTop="40dp"
        		android:text="Create Poll" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
            <Button
        		android:id="@+id/ListInterviews"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
 		        android:layout_alignParentLeft="true"
 		        android:layout_below="@+id/TextView01"
 		        android:layout_marginLeft="23dp"
        		android:layout_marginTop="40dp"
        		android:text="List Interviews" />
            <Button
        		android:id="@+id/ListPolls"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
 		        android:layout_alignParentLeft="true"
 		        android:layout_below="@+id/TextView01"
 		        android:layout_marginLeft="23dp"
        		android:layout_marginTop="40dp"
        		android:text="List Polls" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
            <Button
        		android:id="@+id/GenerateData"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
 		        android:layout_alignParentLeft="true"
 		        android:layout_below="@+id/TextView01"
 		        android:layout_marginLeft="23dp"
        		android:layout_marginTop="40dp"
        		android:text="Generate Data" />
        </TableRow>


        <TableRow
            android:id="@+id/tableRow4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >
        </TableRow>
    </TableLayout>

</RelativeLayout>
